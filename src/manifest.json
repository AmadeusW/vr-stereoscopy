{
  "name": "VR CV",
  "description": "See cross view images in VR.",
  "version": "0.1",
  "permissions": ["<all_urls>"],
  "browser_action": {"default_popup": "popup.html"},
  "manifest_version": 2,
  "content_scripts": [
    {
      "js": ["https://aframe.io/releases/0.4.0/aframe.min.js"]
    }
  ],
  "content_security_policy": "script-src 'self' https://aframe.io; object-src 'self'"
}
